  "main": "index.js",
  "dependencies": {
    "promise-callbacks": "^3.0.0",
    "redis": "^2.7.1"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "ava-spec": "^1.1.0",
    "coveralls": "^2.11.2",
