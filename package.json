{
  "name": "@mixmaxhq/bee-queue",
  "version": "1.0.5",
  "description": "A simple, fast, robust job/task queue, backed by Redis.",
  "main": "index.js",
  "dependencies": {
    "promise-callbacks": "^3.0.0",
    "redis": "^2.7.1"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coveralls": "^2.11.2",
    "grunt": "~0.4.5",
    "grunt-cli": "^1.2.0",
    "grunt-eslint": "20.0.0",
    "grunt-githooks": "^0.3.1",
    "grunt-mocha-test": "^0.12.6",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "sinon": "^1.12.2"
  },
  "scripts": {
    "test": "grunt test",
    "eslint": "grunt eslint",
    "coverage": "istanbul cover _mocha --report lcovonly",
    "publish-coverage": "coveralls < coverage/lcov.info && rm -rf coverage",
    "report": "npm run coverage && istanbul report html",
    "ci": "npm test && npm run coverage && npm run publish-coverage"
  },
  "keywords": [
    "task",
    "job",
    "queue",
    "worker",
    "distributed",
    "system",
    "redis",
    "lua"
  ],
  "engines": {
    "node": ">= 4"
  },
  "author": "Lewis J Ellis <me@lewisjellis.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mixmaxhq/bee-queue.git"
  },
  "bugs": {
    "url": "https://github.com/mixmaxhq/bee-queue/issues"
  },
  "homepage": "https://github.com/mixmaxhq/bee-queue"
}
